<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core" controllerName="Accenutre.AccentureDemo.controller.View1" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
				
						
						
					<Table id="table0" items="{oJsonModel>/}">
				
						
						
					    <items>
				
						
						
					        <ColumnListItem type="Active" id="item0">
				
						
						
					            <cells>
				
						
						
					                <Text text="{oJsonModel>emp_id}" />
				
						
						
					                <Text text="{oJsonModel>emp_name}" />
				
						
						
					                <Text text="{oJsonModel>salary}" /></cells>
				
						
						
					        </ColumnListItem>
				
						
						
					    </items>
				
						
						
					    <columns>
				
						
						
					        <Column >
				
						
						
					            <header>
				
						
						
					                <Label text="Emp No"/>
				
						
						
					            </header>
				
						
						
					        </Column>
				
						
						
					        <Column >
				
						
						
					            <header>
				
						
						
					                <Label text="Emp Name"/>
				
						
						
					            </header>
				
						
						
					        </Column>
				
						
						
					        <Column>
				
						
						
					            <header>
				
						
						
					                <Label text="Salary"/>
				
						
						
					            </header>
				
						
						
					        </Column></columns></Table>
				
						
						
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
